            "shortName": "开发记录"
        },
        "theme-default": {
         	"showLevel": true
    	},
        "github": {
            "url": "https://github.com/bubifengyun/book-yii2-dev-process"
        },
        "editlink": {
            "base": "https://github.com/bubifengyun/book-yii2-dev-process/blob/master/src",
            "label": "编辑本页"
        },
        "atoc": {
            "addClass": true,
            "className": "atoc"
        }
    }
}
