{
    "title": "Yii2 网站开发流程",
    "description": "我们一起在武汉",
    "language": "zh",
    "plugins": [
        "disqus",
        "github",
        "editlink",
        "image-captions",
        "splitter",
        "include-codeblock",
        "ace"
    ],
    "pluginsConfig": {
